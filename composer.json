{
  "name": "netable/netsuite-php",
  "description": "NetSuite PHP API wrapper",
  "keywords": [ "netsuite", "api", "php" ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Syed Saleem",
      "email": "syed@netable.com.au"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-soap": "*",
    "ext-SimpleXML": "*",
    "ext-openssl": "*",
    "paragonie/random_compat": ">=1"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "NetSuite\\": "src/"
    },
    "files": [
      "src/includes/functions.php"
    ]
  },
  "minimum-stability": "stable"
}
